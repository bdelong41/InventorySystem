<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.text.*?>

<AnchorPane prefHeight="150.0" prefWidth="350.0" xmlns="http://javafx.com/javafx/11.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="ConfirmBox">
   <children>
      <Label fx:id="confirm_action_label" alignment="CENTER" layoutX="46.0" layoutY="59.0" prefHeight="25.0" prefWidth="258.0" text="Do you wish to complete this operation?">
         <font>
            <Font size="14.0" />
         </font>
      </Label>
      <Button fx:id="confirmButton" layoutX="58.0" layoutY="104.0" mnemonicParsing="false" onAction="#confirm_button_listener" text="confirm" />
      <Button fx:id="cancelButton" layoutX="244.0" layoutY="104.0" mnemonicParsing="false" onAction="#cancel_button_listener" text="cancel" />
   </children>
</AnchorPane>
